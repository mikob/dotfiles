{
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.focusActiveEditorGroup"
    ],
    "[astro]": {
        "editor.formatOnSave": false
    },
    "terminal.integrated.scrollback": 10000,
    "mjml.previewBackgroundColor": "#ffffff",
    "html.mirrorCursorOnMatchingTag": false,
    "editor.selectionClipboard": false,
    "vim.handleKeys": {
        "<C-w>": false,
        "<C-f>": false,
        "<C-s>": false,
        "<C-c>": false,
        "<C-p>": false,
        "<C-h>": false,
    },
    "vim.easymotion": true,
    "vim.easymotionMarkerBackgroundColor": "#fcba03",
    "vim.easymotionMarkerForegroundColorOneChar": "#000000",
    "vim.easymotionMarkerForegroundColorTwoChar": "#333333",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                {
                    "command": ":wq"
                },
            ]
        },
        {
            "before": [
                "s"
            ],
            "after": [
                "leader",
                "/"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "bookmarks.list"
            ]
        }
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--enable=W0614"
    ],
    "vim.leader": "<space>",
    "explorer.confirmDragAndDrop": false,
    "keyboard.dispatch": "keyCode",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.lineNumbers": "on",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "explorer.autoReveal": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "python.languageServer": "Pylance",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "colorize.languages": [
        "typescript",
        "javascript",
        "css",
        "scss",
        "typescriptreact"
    ],
    "prettier.packageManager": "pnpm",
    "prettier.tabWidth": 4,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "update.mode": "manual",
    "[vue]": {
        "editor.defaultFormatter": "johnsoncodehk.volar"
    },
    "extensions.ignoreRecommendations": false,
    "javascript.autoClosingTags": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "spellright.addToSystemDictionary": true,
    "spellright.notificationClass": "information",
    "spellright.suggestionsInHints": false,
    "gitlens.codeLens.enabled": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "vim.useSystemClipboard": true,
    "vetur.experimental.templateInterpolationService": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.autoDetectColorScheme": true,
    "vetur.format.defaultFormatterOptions": {
        "js-beautify-html": {
            "wrap_attributes": "force-expand-multiline"
        },
        "prettyhtml": {
            "printWidth": 100,
            "singleQuote": false,
            "wrapAttributes": false,
            "sortAttributes": false
        }
    },
    "editor.minimap.enabled": false,
    "vetur.trace.server": "verbose",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "tailwindCSS.includeLanguages": {
        "plaintext": "javascript",
        "typescriptreact": "html",
        "html": "html"
    },
    "html.autoClosingTags": false,
    "typescript.autoClosingTags": false,
    "vetur.useWorkspaceDependencies": true,
    "go.toolsManagement.autoUpdate": true,
    "deno.enable": true,
    "python.formatting.provider": "none",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false,
        "git-commit": false,
        "git-rebase": false
    },
    "volar.icon.preview": false,
    "explorer.confirmDelete": false,
    "npm.packageManager": "pnpm",
    "editor.formatOnSave": true
}