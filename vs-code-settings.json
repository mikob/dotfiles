{
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.focusActiveEditorGroup"
    ],
    "[astro]": {
        "editor.formatOnSave": false
    },
    "terminal.integrated.scrollback": 10000,
    "mjml.previewBackgroundColor": "#ffffff",
    "html.mirrorCursorOnMatchingTag": false,
    "editor.selectionClipboard": false,
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-w>": false,
        "<C-f>": false,
        "<C-s>": false,
        "<C-c>": false,
        "<C-p>": false,
        "<C-h>": false,
        "<C-n>": false,
    },
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.easymotionMarkerBackgroundColor": "#fcba03",
    "vim.easymotionMarkerForegroundColorOneChar": "#000000",
    "vim.easymotionMarkerForegroundColorTwoChar": "#333333",
    "vim.normalModeKeyBindings": [],
    "vim.insertModeKeyBindings": [],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                {
                    "command": ":wq"
                },
            ]
        },
        {
            "before": [
                "s"
            ],
            "after": [
                "leader",
                "/"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "bookmarks.list"
            ]
        }
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--enable=W0614"
    ],
    "explorer.confirmDragAndDrop": false,
    "keyboard.dispatch": "keyCode",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.lineNumbers": "on",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "explorer.autoReveal": false,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "python.languageServer": "Pylance",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "colorize.languages": [
        "typescript",
        "javascript",
        "css",
        "scss",
        "typescriptreact"
    ],
    "prettier.packageManager": "pnpm",
    "prettier.tabWidth": 4,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "update.mode": "manual",
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "extensions.ignoreRecommendations": false,
    "javascript.autoClosingTags": false,
